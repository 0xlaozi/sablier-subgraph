{
  "name": "@sablier/subgraph",
  "version": "0.0.1",
  "author": {
    "name": "Paul Berg",
    "url": "https://paulrberg.com"
  },
  "bugs": {
    "url": "https://github.com/SablierApp/sablier-subgraph/issues"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.10.0",
    "@graphprotocol/graph-ts": "0.10.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SablierApp/sablier-subgraph.git"
  },
  "scripts": {
    "build": "graph build --debug",
    "codegen": "graph codegen --debug --output-dir src/types/",
    "create": "graph create PaulRBerg/sablier-dev --node https://api.thegraph.com/deploy/",
    "create-local": "graph create PaulRBerg/sablier-dev --node http://127.0.0.1:8020",
    "deploy": "graph deploy PaulRBerg/sablier-dev --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy PaulRBerg/sablier-dev --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "remove-local": "graph remove PaulRBerg/sablier-dev --node http://localhost:8020/",
    "reset-local": "yarn codegen && yarn remove-local && yarn create-local && yarn deploy-local"
  },
  "dependencies": {}
}
